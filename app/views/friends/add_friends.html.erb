<% if @search_results.present? %>
  <h2>Search Results</h2>
  <% @search_results.each do |user| %>
    <%= render partial: 'layouts/user_display', locals: { user: user } %>
  <% end %>
<% end %>

<h2>All Users</h2>
<% @users.each do |user| %>
  <%= render partial: 'layouts/user_display', locals: { user: user } %>
<% end %>
